		"@types/node": "^20.0.0",
		"better-auth": "^1.2.0",
		"tsup": "^8.3.5",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"zod": "^3.24.2"
	},
	"peerDependencies": {
		"@polar-sh/sdk": "^0.32.0",
		"better-auth": "^1.2.0"
	}
}
