{
  "name": "@polar-sh/tanstack-start",
  "version": "0.4.3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --format esm,cjs --dts --clean --sourcemap",
    "dev": "tsc --watch",
    "check": "biome check --write ./src"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@polar-sh/adapter-utils": "workspace:*",
    "@polar-sh/sdk": "^0.42.1",
    "@tanstack/react-start": "^1.140.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sindresorhus/tsconfig": "^7.0.0",
    "tsup": "^8.5.1",
    "vitest": "^2.1.8"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
